{
  "name": "crypto_bot",
  "description": "A crypto trading bot",
  "author": "Kristi Jorgji",
  "license": "proprietary",
  "private": true,
  "engines": {
    "node": ">= 22.16.0"
  },
  "scripts": {
    "test": "jest",
    "start": "ts-node -r dotenv/config src/index.ts",
    "dev:cli": "ts-node src/console/cli.ts",
    "server:start": "ts-node -r dotenv/config src/http-api/server.ts",
    "server:dev": "nodemon --config nodemon.server.json",
    "compile": "tsc",
    "lint": "eslint '{src,__tests__}/**/*.ts'",
    "fix": "eslint --fix --ext js,jsx,ts,tsx {src,__tests__}",
    "prepare": "husky",
    "knex:init": "knex init -x ts",
    "knex:run": "node -r dotenv/config node_modules/knex/bin/cli.js --knexfile knexfile.ts",
    "migrate:currentVersion": "npm run knex:run -- migrate:currentVersion",
    "migrate:list": "npm run knex:run -- migrate:list",
    "migrate:make": "npm run knex:run -- migrate:make --",
    "migrate:latest": "npm run knex:run -- migrate:latest --debug",
    "migrate:rollback": "npm run knex:run -- migrate:rollback --all --debug",
    "seed:make": "npm run knex:run -- seed:make --",
    "seed:run": "npm run knex:run -- seed:run --debug",
    "run:script": "ts-node -r tsconfig-paths/register -r dotenv/config",
    "pumpfun-bot": "yarn run:script src/scripts/pumpfun/bot.ts",
    "pumpfun-organize": "yarn run:script src/scripts/pumpfun/organize-stats.ts",
    "pumpfun-migrate-stats": "yarn run:script src/scripts/pumpfun/migrate-stats.ts",
    "pumpfun-migrate-stats-to-db": "yarn run:script src/scripts/pumpfun/migrate-stats-to-db.ts",
    "pumpfun-backtest": "yarn run:script src/scripts/pumpfun/backtest-strategy.ts",
    "pumpfun-backtest-generated-strategies": "yarn run:script src/scripts/pumpfun/backtest-generated-strategies.ts",
    "pumpfun-mark-backtest-files": "yarn run:script src/scripts/pumpfun/mark-backtest-files.ts",
    "pumpfun-validate-backtest-files": "yarn run:script src/scripts/pumpfun/validate-backtest-files.ts",
    "example:pumpfun": "yarn run:script src/examples/pumpfun.ts",
    "example:raydium": "yarn run:script src/examples/raydium.ts",
    "example:exchange_rate": "yarn run:script src/examples/getExchangeRate.ts",
    "example:wallet_balance": "yarn run:script src/examples/getWalletBalance.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@faker-js/faker": "^9.8.0",
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@metaplex-foundation/umi": "^1.2.0",
    "@project-serum/anchor": "^0.26.0",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.58",
    "@raydium-io/raydium-sdk-v2": "^0.1.138-alpha",
    "@solana/buffer-layout": "^4.0.1",
    "@solana/buffer-layout-utils": "^0.2.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.2",
    "axios": "^1.9.0",
    "axios-rate-limit": "^1.4.0",
    "bcryptjs": "^3.0.2",
    "big-json": "^3.2.0",
    "bip39": "^3.1.0",
    "bn.js": "^5.2.2",
    "bs58": "^6.0.0",
    "commander": "^14.0.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ed25519-hd-key": "^1.3.0",
    "express": "^5.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "logform": "^2.7.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.1",
    "opossum": "^8.4.0",
    "pako": "^2.1.0",
    "parse-duration": "^1.1.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-transport": "^4.9.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@kristijorgji/eslint-config-typescript": "^0.0.3",
    "@types/big-json": "^3.2.5",
    "@types/bn.js": "^5.1.6",
    "@types/callsite": "^1.0.34",
    "@types/compression": "^1.8.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/ioredis-mock": "^8.2.6",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.24",
    "@types/opossum": "^8.1.8",
    "@types/pako": "^2.0.3",
    "@types/supertest": "^6.0.3",
    "callsite": "^1.0.0",
    "husky": "^9.1.7",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "jest-websocket-mock": "^2.5.0",
    "mock-socket": "^9.3.1",
    "msw": "^2.8.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
