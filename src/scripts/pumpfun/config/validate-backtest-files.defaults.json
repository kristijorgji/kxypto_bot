{
  "dataSource": {
    "path": "data/pumpfun-stats/tmp"
  },
  "rules": {
    "notJson": true,
    "nulls": {
      "enabled": true,
      "exclude": {
        "devHoldingPercentageCirculating": {},
        "topTenHoldingPercentageCirculating": {},
        "topHolderCirculatingPercentage": {}
      }
    },
    "noHistory": true,
    "minHistoryLength": 40
  },
  "extractTo": "data/invalid-pumpfun-stats",
  "reportPath": "data/validate-backtest-files-report.json"
}
